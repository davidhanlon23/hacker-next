{
  "name": "hacker-next",
  "version": "1.0.0",
  "description": "A Hacker News replica",
  "main": "server.js",
  "scripts": {
    "dev": "node server.js",
    "start": "set NODE_ENV=production && node server.js",
    "build": "next build",
    "export": "next export",
    "deploy": "cd out && now --public",
    "now-build": "next build && next export -o dist",
    "heroku-postbuild":"next build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davidhanlon23/hacker-next.git"
  },
