  "bugs": {
    "url": "https://github.com/davidhanlon23/hacker-next/issues"
  },
  "homepage": "https://github.com/davidhanlon23/hacker-next#readme",
  "dependencies": {
    "isomorphic-unfetch": "^3.0.0",
    "next": "^9.0.3",
    "nprogress": "^0.2.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "sw-precache-webpack-plugin": "^0.11.5"
  },
  "devDependencies": {
    "@babel/preset-stage-0": "^7.0.0",
    "babel-eslint": "^10.0.2",
    "eslint": "^6.1.0",
    "eslint-config-google": "^0.13.0",
    "eslint-plugin-react": "^7.14.3"
  }
